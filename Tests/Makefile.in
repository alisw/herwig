	cd Rivet-$(notdir $(subst .in,,$<)); echo `pwd`; \
	../$(HERWIG) read --repo ../$(REPO) -L ../$(top_builddir)/lib -i ../$(top_builddir)/src ../$<; \
	../$(HERWIG) run $(notdir $(subst .in,.run,$<)) -N $${NUMEVENTS:-10000}; \
	mv $(notdir $(subst .in,.yoda,$<)) ..; \
	cd ..

